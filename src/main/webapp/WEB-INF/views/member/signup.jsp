<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<html>
<head>
	<title>capstone</title>
</head>
<body>

			
<section id="content">
	<form role="form" method="post" autocomplete="off">
		<div class="input_area">
			<label for="name">이름</label>
			<input type="text" id="name" name="name" placeholder="네임" required="required" />						
		</div>
		
		<div class="input_area">
			<label for="id">아이디</label>
			<input type="text" id="id" name="id" placeholder="id" required="required" />						
		</div>
		
		<div class="input_area">
			<label for="pw">패스워드</label>
			<input type="password" id="pw" name="pw" placeholder="1234" required="required" />						
		</div>
		
		<div class="input_area">
			<label for="major">전공</label>
			<input type="text" id="major" name="id" placeholder="컴퓨터공학과" required="required" />						
		</div>
		
		<div class="input_area">
			<label for="student_num">학번</label>
			<input type="text" id="student_num" name="student_num" placeholder="2017000000" required="required" />						
		</div>
		
		<div class="input_area">
			<label for="email">이메일</label>
			<input type="text" id="email" name="email" placeholder="create@naver.com" required="required" />						
		</div>
		
		<div class="input_area">
			<label for="phone_number">연락처</label>
			<input type="text" id="phone_number" name="phone_number" placeholder="010-0000-0000" required="required" />						
		</div>
		
		<button type="submit" id="signup_btn" name="signup_btn">회원가입</button>
		
	</form>			
</section>
			
		</div>
	</section>

</div>
</body>
</html>
